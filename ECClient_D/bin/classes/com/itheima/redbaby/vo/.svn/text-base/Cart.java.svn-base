package com.itheima.redbaby.vo;

import java.util.ArrayList;
import java.util.List;


public class Cart {
	/** 商品列表 */
	public List<CartProduct> productlist = new ArrayList<CartProduct>();
	
	/** 享受促销信息 */
	public List<String> cart_prom = new ArrayList<String>();
	
	/** 购物车总计*/
	public Addup cart_addup;

	public Cart() {

	}

	public Cart(List<CartProduct> productlist, List<String> cart_prom, Addup cart_addup) {
		super();
		this.productlist = productlist;
		this.cart_prom = cart_prom;
		this.cart_addup = cart_addup;
	}

	public List<CartProduct> getProductlist() {
		return productlist;
	}

	public void setProductlist(List<CartProduct> productlist) {
		this.productlist = productlist;
	}

	public List<String> getCart_prom() {
		return cart_prom;
	}

	public void setCart_prom(List<String> cart_prom) {
		this.cart_prom = cart_prom;
	}

	public Addup getCart_addup() {
		return cart_addup;
	}

	public void setCart_addup(Addup cart_addup) {
		this.cart_addup = cart_addup;
	}
}
